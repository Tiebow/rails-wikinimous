<%= link_to 'Wikinimous', articles_path %>
<div class="container">
<h1>Editing an article</h1>
<%= form_for(@article) do |f| %>
<%= f.label :title %>
<%= f.text_field :title %><br>
<%= f.label :content %>
<%= f.text_field :content %><br>
<%= f.submit %>
<% end %>
<%= link_to 'Show', article_path(@article) %>
<%= link_to 'Back', articles_path %>
</div>
